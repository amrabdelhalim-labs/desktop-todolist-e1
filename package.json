  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/images/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . todo-list-app --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/images/icon.ico --prune=true --out=release-builds",
    "package-linux": "electron-packager . todo-list-app --overwrite --asar --platform=linux --arch=x64 --icon=assets/images/icon.png --prune=true --out=release-builds"
  },
  "devDependencies": {
    "electron": "^39.2.3"
  },
  "dependencies": {
    "electron-packager": "^17.1.2",
    "jsstore": "^3.12.0",
    "sass": "^1.94.2"
  }
